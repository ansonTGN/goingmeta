@prefix mv: <http://www.neo4j.org/sch/movies#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix talk: <http://www.neo4j.org/2022/07/talkable#> .
@base <http://www.w3.org/2002/07/owl#> .

[ rdf:type owl:Ontology
 ] .

#################################################################
#    Annotation properties
#################################################################

###  http://www.neo4j.org/2022/07/talkable#direct
talk:direct rdf:type owl:AnnotationProperty .


###  http://www.neo4j.org/2022/07/talkable#inverse
talk:inverse rdf:type owl:AnnotationProperty .


###  http://www.neo4j.org/2022/07/talkable#name
talk:name rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  neo4j://www.neo4j.org/sch/movies#ACTED_IN
mv:ACTED_IN rdf:type owl:ObjectProperty ;
            rdfs:domain mv:Person ;
            rdfs:range mv:Movie ;
            talk:direct "acted in"@default ,
                        "is in the cast of"@long ,
                        "worked in"@short ;
            talk:inverse "has $o in it"@default ,
                         "includes"@short ,
                         "includes $o in the cast"@long ;
            rdfs:label "ACTED_IN" .


###  neo4j://www.neo4j.org/sch/movies#WROTE
mv:WROTE rdf:type owl:ObjectProperty ;
         rdfs:domain mv:Person ;
         rdfs:range mv:Movie ;
         talk:direct "is the author of"@default ,
                     "wrote"@short ,
                     "wrote the script of"@long ;
         talk:inverse "is authored by"@default ,
                      "is written by"@long ,
                      "is by"@short ;
         rdfs:label "WROTE" .


#################################################################
#    Data properties
#################################################################

###  neo4j://www.neo4j.org/sch/movies#born
mv:born rdf:type owl:DatatypeProperty ;
        rdfs:domain mv:Person ;
        talk:direct "The birth year of $s was"@long ,
                    "spawned in"@short ,
                    "was born in"@default ;
        rdfs:label "born" .


###  neo4j://www.neo4j.org/sch/movies#released
mv:released rdf:type owl:DatatypeProperty ;
            rdfs:domain mv:Movie ;
            talk:direct "premiered in"@short ,
                        "The release year of $s was"@long ,
                        "was released in"@default ;
            rdfs:label "released" .


###  neo4j://www.neo4j.org/sch/movies#tagline
mv:tagline rdf:type owl:DatatypeProperty ;
           rdfs:domain mv:Movie ;
           talk:direct  "'s tagline goes: '$o'"@default ,
           				"'s tagline is"@short ,
                       "the tagline for $s was"@long ;
           rdfs:label "tagline" .


#################################################################
#    Classes
#################################################################

###  neo4j://www.neo4j.org/sch/movies#Movie
mv:Movie rdf:type owl:Class ;
         talk:name "title" ;
         rdfs:label "Movie" .


###  neo4j://www.neo4j.org/sch/movies#Person
mv:Person rdf:type owl:Class ;
          talk:name "name" ;
          rdfs:label "Person" .


